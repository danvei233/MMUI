<script>
 
    let sunid, suncode, todeskid, todeskcode = '获取中...';
    
    $(document).ready(function() {
        const hostId = '228';
        const $loaderLIST = $('#loaderLIST');
    
        $loaderLIST.show(); // 显示加载动画
        console.log('开始加载DVAPI数据...');
        $.post(rootUrl + 'getDVapi', { hostid: hostId, action: 'get' }, function(data) {
            $loaderLIST.hide(); // 隐藏加载动画
            console.log('DVAPI数据请求完成:', data);
            if (data.code === 200) {
                const response = data.response;
                const $deviceDetails = $('#device-details');
                $deviceDetails.empty(); // 清空之前的内容
    
                response.forEach(device => {
                    const appName = device['应用名称'] || '未知应用';
                    let todayCode = device['今日验证码'] || '无';
                    let deviceId = device['设备识别码'] ? device['设备识别码'][0] : '无';
                    const appShortName = appName === '向日葵' ? 'SUN' : appName === 'ToDesk' ? 'TD' : '未知';
    
                    // Apply default codes
                    if (appShortName === 'SUN') {
                        if (todayCode === '无') {
                            todayCode = '系统异常';
                        }
                        if (deviceId.startsWith('k')) {
                            deviceId = deviceId.substring(1);
                        }
                        sunid = deviceId;
                        suncode = todayCode;
                    } else if (appShortName === 'TD') {
                        if (todayCode === '无') {
                            todayCode = '系统异常';
                        }
                        todeskid = deviceId;
                        todeskcode = todayCode;
                    }
    
                    let listItem = `
                        <li class="mdui-list-item mdui-ripple" ondblclick="copyToClip('${deviceId}')">
                            ${appShortName} 识别码：${deviceId}
                        </li>
                        <li class="mdui-list-item mdui-ripple" ondblclick="copyToClip('${todayCode}')">
                            ${appShortName} 验证码：${todayCode}  
                            <a style="height=3px;margin-left: auto;" onclick="reloaddvapi()" class="reload-dvapi cada mdui-btn mdui-btn-raised mdui-ripple mdui-color-theme-accent mdui-btn-icon" mdui-tooltip="{content: '新刷信息',delay: 300, position: 'top'}">
                                <i class="mdui-icon material-icons">refresh</i>
                            </a>
                        </li>
                    `;
    
                    $deviceDetails.append(listItem);
                });
            } else {
                console.error('Error:', data.msg);
            }
        }, 'json');
    });
    
    function reloaddvapi() {
        let $ = layui.jquery;
        const hostId = '228';
        const $loaderLIST = $('#loaderLIST');
    
        $loaderLIST.show(); // 显示加载动画
        console.log('开始加载DVAPI数据...');
