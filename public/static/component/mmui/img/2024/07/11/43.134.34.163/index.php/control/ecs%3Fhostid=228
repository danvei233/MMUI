            $loaderLIST.hide(); // 隐藏加载动画
            console.log('DVAPI数据请求完成:', data);
            if (data.code === 200) {
                const response = data.response;
                const $deviceDetails = $('#device-details');
                $deviceDetails.empty(); // 清空之前的内容
    
                response.forEach(device => {
                    const appName = device['应用名称'] || '未知应用';
                    let todayCode = device['今日验证码'] || '无';
                    let deviceId = device['设备识别码'] ? device['设备识别码'][0] : '无';
                    const appShortName = appName === '向日葵' ? 'SUN' : appName === 'ToDesk' ? 'TD' : '未知';
    
                    // Apply default codes
                    if (appShortName === 'SUN') {
                        if (todayCode === '无') {
                            todayCode = '系统异常';
                        }
                        if (deviceId.startsWith('k')) {
                            deviceId = deviceId.substring(1);
                        }
                        sunid = deviceId;
                        suncode = todayCode;
                    } else if (appShortName === 'TD') {
                        if (todayCode === '无') {
                            todayCode = '系统异常';
                        }
                        todeskid = deviceId;
                        todeskcode = todayCode;
                    }
    
