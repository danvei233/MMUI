                layer.closeAll('loading');
                if (data.code == 1) {
                    var data = data.data;
                    
                    $('#status').html(data[1]);
                    if ($('#status').html() === "运行中") {
    // 如果内容是"运行中"，则使具有ptc类的按钮可见
                                 $('.ptc').show();
                                 $('.stc').hide();
            } else {
    // 否则，隐藏这些按钮
                      $('.ptc').hide();
                      $('.stc').show();
                        }
                    return false;
                } else {
                    $('#status').html(data.msg);
                }
            },'json');
